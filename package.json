{
  "name": "react-modal-message-wrapper",
  "version": "0.1.3",
  "description": "A thin wrapper around rect-bootstraps modal message",
  "main": "dist/react-modal-message-wrapper.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register --recursive --require test/setup.js",
    "lint": "eslint src/modal.jsx",
    "build": "webpack",
    "test-coverage": "babel-node ./node_modules/.bin/babel-istanbul cover _mocha -- --require test/setup.js --compilers js:babel-register test/*.spec.js && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zalando/react-modal-message-wrapper.git"
  },
  "keywords": [
    "react",
    "react-bootstrap",
    "modal"
  ],
  "author": "Jan Stroppel <jan.stroppel@zalando.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zalando/react-modal-message-wrapper/issues"
  },
  "homepage": "https://github.com/zalando/react-modal-message-wrapper",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "devDependencies": {
    "babel-cli": "6.7.5",
    "babel-core": "6.7.4",
    "babel-eslint": "6.0.2",
    "babel-istanbul": "0.7.0",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-register": "6.7.2",
    "coveralls": "2.11.9",
    "eslint": "2.7.0",
    "eslint-loader": "1.3.0",
    "eslint-plugin-react": "4.2.3",
    "expect": "1.14.0",
    "istanbul": "0.4.3",
    "jsdom": "8.3.1",
    "mocha": "2.4.5",
    "react": "0.14.8",
    "react-addons-test-utils": "0.14.8",
    "react-bootstrap": "0.28.3",
    "react-dom": "0.14.8",
    "style-loader": "0.13.1",
    "webpack": "1.12.14"
  }
}
