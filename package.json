{
  "name": "react-modal-message-wrapper",
  "version": "0.3.2",
  "description": "A thin wrapper around react-bootstraps modal message",
  "main": "dist/react-modal-message-wrapper.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register --recursive --require test/setup.js",
    "lint": "eslint src/modal.jsx",
    "build": "webpack",
    "test-coverage": "babel-node ./node_modules/.bin/babel-istanbul cover _mocha -- --require test/setup.js --compilers js:babel-register test/*.spec.js && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jans510/react-modal-message-wrapper.git"
  },
  "keywords": [
    "react",
    "react-bootstrap",
    "modal"
  ],
  "author": "Jan Stroppel <jan.stroppel@zalando.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jans510/react-modal-message-wrapper/issues"
  },
  "homepage": "https://github.com/jans510/react-modal-message-wrapper",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "peerDependencies": {
    "react": ">=0.14.0",
    "react-bootstrap": ">=0.28.0"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.3",
    "babel-istanbul": "^0.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.8.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.9.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^5.0.1",
    "expect": "^1.19.0",
    "istanbul": "^0.4.3",
    "jsdom": "^8.5.0",
    "mocha": "^2.4.5",
    "react": "^15.0.1",
    "react-addons-test-utils": "^15.0.1",
    "react-bootstrap": "^0.29.0",
    "react-dom": "^15.0.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0"
  }
}
